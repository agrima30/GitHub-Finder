{\n  const githubContext = useContext(GithubContext);\n  const alertContext = useContext(AlertContext);\n  const [text, setText] = useState('');\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n    if (text === '') {\n      alertContext.setAlert('Please enter something', 'light');\n    } else {\n      githubContext.searchUsers(text);\n      setText('');\n    }\n  };\n\n  const onChange = (e) => setText(e.target.value);\n\n  return (\n    <div>\n      <form onSubmit={onSubmit} className='form'>\n        <input\n          type='text'\n          name='text'\n          placeholder='Search Users ....'\n          value={text}\n          onChange={onChange}\n        />\n        <input\n          type='submit'\n          value='Search'\n          className='btn btn-dark btn-block'\n        />\n      </form>\n      {githubContext.users.length > 0 && (\n        <button\n          class='btn btn-dark btn-block'\n          onClick={githubContext.clearUsers}\n        >\n          Clear\n        </button>\n      )}\n    </div>\n  );\n};\n\nexport default Search;\n","/home/agu/Documents/React/react udemy/github-finder/src/Components/Layout/Alert.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/Components/pages/About.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/Components/users/User.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/Components/repos/Repos.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/Components/repos/RepoItem.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/context/github/GithubState.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/context/github/githubContext.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/context/types.js",[],"/home/agu/Documents/React/react udemy/github-finder/src/context/github/githubReducer.js",["188"],"import {\n  SEARCH_USERS,\n  SET_LOADING,\n  CLEAR_USERS,\n  GET_USER,\n  GET_REPOS,\n} from '../types';\n\nexport default (s